journal = {Nature},
keywords = {*Models, Neurological,Animals,Biomechanical Phenomena,Electromyography,Leeches,Macaca mulatta/*physiology,Male,Motor Cortex/*cytology/*physiology,Movement/*physiology,Neurons/*cytology,Rotation,Swimming,Walking},
