pages = {1450--1456},
pmid = {24730718},
title = {{Negative affect improves the quality of memories: Trading capacity for precision in sensory and working memory.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/24730718},
volume = {143},
year = {2014}
}
