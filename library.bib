journal = {Memory (Hove, England)},
keywords = {Adult,Anger,Depression,Depression: psychology,Emotions,Facial Expression,Female,Happiness,Humans,Male,Memory, Short-Term,Neuropsychological Tests,Recognition (Psychology)},
month = {jul},
number = {5},
