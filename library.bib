journal = {J Cogn Neurosci},
number = {4},
pages = {311--327},
pmid = {23971503},
title = {{Modeling the Temporal Dynamics of IT Neurons in Visual Search: A Mechanism for Top-Down Selective Attention}},
volume = {8},
year = {1996}
}
