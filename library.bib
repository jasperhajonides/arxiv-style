doi = {10.1016/j.cognition.2012.10.002},
file = {:Users/jasperhajonides/Library/Application Support/Mendeley Desktop/Downloaded/Pe, Koval, Kuppens - 2013 - Executive well-being Updating of positive stimuli in working memory is associated with subjective well-being.pdf:pdf},
isbn = {0010-0277},
