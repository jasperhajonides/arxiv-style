file = {:Users/jasperhajonides/Library/Application Support/Mendeley Desktop/Downloaded/Koster et al. - 2007 - A time-course analysis of attentional cueing by threatening scenes.pdf:pdf},
isbn = {1618-3169 (Print)\r1618-3169 (Linking)},
