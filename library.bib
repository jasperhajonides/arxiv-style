journal = {Visual Neuroscience},
keywords = {eccentricity,primate,supra-areal,topography,visual cortex},
pages = {E014},
pmid = {26241035},
title = {{Topographic organization of areas V3 and V4 and its relation to supra-areal organization of the primate visual system}},
url = {http://www.journals.cambridge.org/abstract_S0952523815000115},
volume = {32},
year = {2015}
}
