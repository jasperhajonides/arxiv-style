pmid = {6470767},
title = {{Stimulus-selective neurons in the macaque}},
volume = {4},
year = {1984}
}
@article{Phillips2008,
abstract = {Decoding facial expressions of emotion is an important aspect of social communication that is often impaired following psychiatric or neurological illness. However, little is known of the cognitive components involved in perceiving emotional expressions. Three dual task studies explored the role of verbal working memory in decoding emotions. Concurrent working memory load substantially interfered with choosing which emotional label described a facial expression (Experiment 1). A key factor in the magnitude of interference was the number of emotion labels from which to choose (Experiment 2). In contrast the ability to decide that two faces represented the same emotion in a discrimination task was relatively unaffected by concurrent working memory load (Experiment 3). Different methods of assessing emotion perception make substantially different demands on working memory. Implications for clinical disorders which affect both working memory and emotion perception are considered.},
author = {Phillips, Louise H and Channon, Shelley and Tunstall, Mary and Hedenstrom, Anna and Lyons, Kathryn},
doi = {10.1037/1528-3542.8.2.184},
file = {:Users/jasperhajonides/Library/Application Support/Mendeley Desktop/Downloaded/Phillips et al. - 2008 - The role of working memory in decoding emotions.pdf:pdf},
isbn = {1528-3542},
issn = {1528-3542},
journal = {Emotion (Washington, D.C.)},
keywords = {anezou-,are very salient cues,bozikas,communication,cues can impair social,e,emotion perception,facial expressions,facial expressions of emotion,functioning and quality of,g,in social,inability to decode these,kosmidis,life,working memory},
number = {2},
pages = {184--191},
pmid = {18410192},
title = {{The role of working memory in decoding emotions.}},
