@article{VanEekelen2003,
abstract = {Time course in task performance has been studied extensively. In particular, the investigation of circadian rhythmicity in task performance that varied in complexity. However, these studies disclosed heterogeneous outcomes. This could be the result of confounding exogenous factors, the use of diverse tasks, as well as accumulating sleepiness interfering with the underlying circadian drive. The present study varied task demands systematically within a single task and a dual task, using a constant routine protocol to examine the unmasked influence of the endogenous circadian oscillator on the periodicity of performance. Moreover, the subjects were divided into an early-start and a late-start group to estimate the potential interaction of circadian rhythmicity with the duration of prior wakefulness. The results revealed a distinct congruence in the circadian rhythms of all performance measures, with which prior wakefulness (< 40 h) did not interact. Also, single-task as well as dual-task complexity did not interfere with circadian rhythmicity. In conclusion, when sufficiently controlled for masking exogenous factors, task complexity is removed from the underlying circadian rhythms.},
author = {van Eekelen, Alexander P J and Kerkhof, Gerard},
doi = {10.1080/0014013031000121598},
file = {:Users/jasperhajonides/Library/Application Support/Mendeley Desktop/Downloaded/van Eekelen, Kerkhof - 2003 - No interference of task complexity with circadian rhythmicity in a constant routine protocol.pdf:pdf},
