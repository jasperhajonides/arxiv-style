journal = {The Journal of neuroscience : the official journal of the Society for Neuroscience},
keywords = {Adult,Brain Mapping,Calibration,Female,Fourier Analysis,Humans,Magnetic Resonance Imaging,Male,Motion Perception,Motion Perception: physiology,Reference Values,Retina,Retina: physiology,Species Specificity,Temporal Lobe,Temporal Lobe: anatomy & histology,Temporal Lobe: physiology,Visual Cortex,Visual Cortex: anatomy & histology,Visual Cortex: physiology,Visual Fields,Visual Fields: physiology,Visual Pathways,Visual Pathways: anatomy & histology,Visual Pathways: physiology,Visual Perception,Visual Perception: physiology,Young Adult},
