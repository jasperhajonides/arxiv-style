volume = {453},
year = {2008}
}
@article{Kang2020,
abstract = {Previous work has shown bidirectional crosstalk between Working Memory (WM) and perception such that the contents of WM can alter what we perceive and vice versa. However, it remains unclear to what extent such interactions reflect changes in sensory and mnemonic signals and/or biases in their decisional evaluation and subjective reporting. Here, using an extended visuospatial number integration task, we report evidence for both sensory and decisional WM-perception interactions and show that they appear to be functionally independent. Spatiotemporally resolved psychometrics during concurrent WM maintenance disclosed (i) a brief visuospatial gain modulation after attending the WM information and (ii) categorical choice biases, in both perceptual and mnemonic decisions, after prolonged unattended storage. The findings highlight a role of decisional processing in behaviorally relevant WM-perception interactions and support a view that attended and unattended WM states may rely on representation at different levels of abstraction.

### Competing Interest Statement

The authors have declared no competing interest.},
author = {Kang, Zhiqi and Spitzer, Bernhard},
doi = {10.1101/2020.07.16.191445},
file = {:Users/jasperhajonides/Dropbox (Attention Group)/Attention Group Team Folder/Myers/MemControl/literature/2020.07.16.191445v2.full.pdf:pdf},
issn = {2692-8205},
journal = {bioRxiv},
