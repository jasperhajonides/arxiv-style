volume = {198},
year = {2012}
}
@article{Lepsien2011,
abstract = {Many current models of working memory (WM) emphasize a close relationship between WM and attention. Recently it was demonstrated that attention can be dynamically and voluntarily oriented to items held in WM, and it was suggested that directed attention can modulate the maintenance of specific WM representations. Here we used event-related functional magnetic resonance imaging to test the effects of orienting attention to a category of stimuli when participants maintained a variable number of faces and scenes in WM. Retro-cues that indicated the relevant stimulus type for the subsequent WM test modulated maintenance-related activity in extrastriate areas preferentially responsive to face or scene stimuli - fusiform and parahippocampal gyri respectively - in a categorical way. After the retro-cue, the activity level in these areas was larger for the cued category in a load-independent way, suggesting the modulation may also reflect anticipation of the probe stimulus. Activity in associative parietal and prefrontal cortices was also modulated by retro-cues, and additionally co-varied with the number of stimuli of the relevant stimulus category that was being maintained. The findings suggest that these associative areas participate in maintaining the relevant memoranda in a flexible and goal-directed way to guide future behaviour. {\textcopyright} 2011 Elsevier Ltd.},
author = {Lepsien, J{\"{o}}ran and Thornton, Ian and Nobre, Anna C.},
doi = {10.1016/j.neuropsychologia.2011.03.011},
file = {:Users/jasperhajonides/Library/Application Support/Mendeley Desktop/Downloaded/Lepsien, Thornton, Nobre - 2011 - Neuropsychologia Modulation of working-memory maintenance by directed attention.pdf:pdf},
issn = {00283932},
journal = {Neuropsychologia},
keywords = {Attention,FMRI,Maintenance,Mental representations,Working memory},
number = {6},
pages = {1569--1577},
pmid = {21420421},
title = {{Modulation of working-memory maintenance by directed attention}},
volume = {49},
year = {2011}
}
