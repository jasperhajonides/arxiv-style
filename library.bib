issn = {15461726},
journal = {Nature Neuroscience},
number = {1},
pages = {150--157},
pmid = {26595654},
title = {{Decoding the content of visual short-term memory under distraction in occipital and parietal areas}},
volume = {19},
year = {2015}
}
@article{Huang2020,
abstract = {Previous studies have shown that when there are statistical regularities in the items stored in visual working memory, the responses are biased toward the ensemble average. This statistical-regularity-induced bias could happen in two ways: (1) a target bias, where the individual memory representations are pulled toward the ensemble average; or (2) a strategic guess, for items that are not memorized, other information in the ensemble (e.g., another item) is reported as a substitute. Here, these two mechanisms are distinguished on the basis of a three-part model (target responses + swap responses + random guesses; e.g., Bays, Catalao, & Husain, 2009, Journal of Vision, 9, 7). The strategic guess is operationalized as swap responses, whereas the target bias is reflected by a bias parameter in the target responses. This model was applied on 8 data sets (22 observers each). In this model, contributions of target biases and strategic guesses can be clearly distinguished from each other because they lead to distinctive patterns in the distribution of responses. In the present results, strategic guesses always contributed substantially to the statistical-regularity-induced biases, whereas target biases were limited to specific conditions. All in all, the Bayesian inference in visual working memory is much more limited than what is previously advocated.},
author = {Huang, Liqiang},
doi = {10.3758/s13414-019-01913-2},
file = {:Users/jasperhajonides/Library/Application Support/Mendeley Desktop/Downloaded/Huang - 2020 - Distinguishing target biases and strategic guesses in visual working memory.pdf:pdf},
isbn = {1341401901},
issn = {1943393X},
journal = {Attention, Perception, and Psychophysics},
keywords = {Ensemble,Memory bias,Working memory},
number = {3},
pages = {1258--1270},
pmid = {31758526},
publisher = {Attention, Perception, & Psychophysics},
title = {{Distinguishing target biases and strategic guesses in visual working memory}},
volume = {82},
year = {2020}
}
@article{Friedman2000,
abstract = {As event-related brain potential (ERP) researchers have increased the number of recording sites, they have gained further insights into the electrical activity in the neural networks underlying explicit memory. A review of the results of such ERP mapping studies suggests that there is good correspondence between ERP results and those from brain imaging studies that map hemodynamic changes. This concordance is important because the combination of the high temporal resolution of ERPs with the high spatial resolution of hemodynamic imaging methods will provide a greatly increased understanding of the spatio-temporal dynamics of the brain networks that encode and retrieve explicit memories. (C) 2000 Wiley-Liss, Inc.},
author = {Friedman, David and Johnson, Ray},
