@article{Cooper2008a,
abstract = {Previous work has suggested that elevated levels of trait anxiety are associated with an increased ability to accurately recognize the facial expression of fear. However, to date, recognition has only been assessed after viewing periods of 10 s, despite the fact that the process of emotion recognition from faces typically takes a fraction of this time. The current study required participants with either high or low levels of non-clinical trait anxiety to make speeded emotional classification judgments to a series of facial expressions drawn from seven emotional categories. Following previous work it was predicted that recognition of fearful facial expressions would be more accurate in the high-trait anxious group compared with the low-trait anxious group. However, contrary to this prediction, no anxiety-related differences in emotion perception were observed across all seven emotions. This suggests that anxiety does not influence the perception of fear as has been previously proposed. ?? 2007 Elsevier Ltd. All rights reserved.},
