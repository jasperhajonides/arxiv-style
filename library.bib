journal = {Cogn Neuropsychol},
keywords = {*Circadian Rhythm,*Cognition,*Thinking,Adult,Aged,Aging/*psychology,Arousal,Attention,Humans,Individuality,Inhibition (Psychology),Memory,Middle Aged,Neuropsychological Tests,Problem Solving,Psychomotor Performance,Sleep,Wakefulness},
number = {7},
pages = {755--789},
pmid = {18066734},
title = {{A time to think: circadian rhythms in human cognition}},
