}
@article{Ye2016,
abstract = {In visual working memory (VWM) tasks, participants' performance can be improved by a retro-object-cue. However, previous studies have not investigated whether participants' performance can also be improved by a retro-dimension-cue. Three experiments investigated this issue. We used a recall task with a retro-dimension-cue in all experiments. In Experiment 1, we found benefits from retro-dimension-cues compared to neutral cues. This retro-dimension-cue benefit is reflected in an increased probability of reporting the target, but not in the probability of reporting the non-target, as well as increased precision with which this item is remembered. Experiment 2 replicated the retro-dimension-cue benefit and showed that the length of the blank interval after the cue disappeared did not influence recall performance. Experiment 3 replicated the results of Experiment 2 with a lower memory load. Our studies provide evidence that there is a robust retro-dimension-cue benefit in VWM. Participants can use internal attention to flexibly allocate cognitive resources to a particular dimension of memory representations. The results also support the feature-based storing hypothesis.},
author = {Ye, Chaoxiong and Hu, Zhonghua and Ristaniemi, Tapani and Gendron, Maria and Liu, Qiang},
doi = {10.1038/srep35573},
file = {:Users/jasperhajonides/Library/Application Support/Mendeley Desktop/Downloaded/Ye et al. - 2016 - Retro-dimension-cue benefit in visual working memory.pdf:pdf},
issn = {20452322},
journal = {Scientific Reports},
pages = {1--13},
pmid = {27774983},
publisher = {Nature Publishing Group},
title = {{Retro-dimension-cue benefit in visual working memory}},
url = {http://dx.doi.org/10.1038/srep35573},
volume = {6},
year = {2016}
}
