journal = {Attention, Perception, and Psychophysics},
keywords = {Attention,Attraction,Interaction,Repulsion,Working memory},
number = {8},
pages = {2376--2395},
pmid = {28836145},
publisher = {Attention, Perception, & Psychophysics},
title = {{Interactions between visual working memory representations}},
volume = {79},
year = {2017}
}
@article{Lorenc2018,
abstract = {Visual working memory (VWM) recruits a broad network of brain regions, including prefrontal, parietal, and visual cortices. Recent evidence supports a “sensory recruitment” model of VWM, whereby precise visual details are maintained in the same stimulus-selective regions responsible for perception. A key question in evaluating the sensory recruitment model is how VWM representations persist through distracting visual input, given that the early visual areas that putatively represent VWM content are susceptible to interference from visual stimulation. To address this question, we used a functional magnetic resonance imaging inverted encoding model approach to quantitatively assess the effect of distractors on VWM representations in early visual cortex and the intraparietal sulcus (IPS), another region previously implicated in the storage of VWM information. This approach allowed us to reconstruct VWM representations for orientation, both before and after visual interference, and to examine whether oriented distractors systematically biased these representations. In our human participants (both male and female), we found that orientation information was maintained simultaneously in early visual areas and IPS in anticipation of possible distraction, and these representations persisted in the absence of distraction. Importantly, early visual representations were susceptible to interference; VWM orientations reconstructed from visual cortex were significantly biased toward distractors, corresponding to a small attractive bias in behavior. In contrast, IPS representations did not show such a bias. These results provide quantitative insight into the effect of interference on VWM representations, and they suggest a dynamic tradeoff between visual and parietal regions that allows flexible adaptation to task demands in service of VWM.},
author = {Lorenc, Elizabeth S. and Sreenivasan, Kartik K. and Nee, Derek E. and Vandenbroucke, Annelinde R.E. and D'Esposito, Mark},
doi = {10.1523/JNEUROSCI.3061-17.2018},
file = {:Users/jasperhajonides/Library/Application Support/Mendeley Desktop/Downloaded/Lorenc et al. - 2018 - Flexible coding of visual working memory representations during distraction.pdf:pdf},
issn = {15292401},
journal = {Journal of Neuroscience},
keywords = {Distraction,Sensory recruitment,Visual working memory},
number = {23},
pages = {5267--5276},
pmid = {29739867},
title = {{Flexible coding of visual working memory representations during distraction}},
volume = {38},
year = {2018}
}
@article{Sotres-Bayon2006a,
abstract = {What brain regions are involved in regulating behavior when the emotional consequence of a stimulus changes from harmful to harmless? One way to address this question is to study the neural mechanisms underlying extinction of Pavlovian fear conditioning, an important form of emotional regulation that has direct relevance to the treatment of human fear and anxiety disorders. In fear extinction, the capacity of a conditioned stimulus to elicit fear is gradually reduced by repeatedly presenting it in the absence of any aversive consequence. In recent years there has been a dramatic increase in research on the brain mechanisms of fear extinction. One region that has received considerable attention as a component of the brain's extinction circuitry is the medial prefrontal cortex (mPFC). In the present article, we review the historical foundations of the modern notion that the mPFC plays a critical role in emotional regulation, a literature that was largely responsible for studies that explored the role of the mPFC in fear extinction. We also consider the role of the mPFC in a broader neural circuit for extinction that includes the amygdala and hippocampus. ?? 2006 Society of Biological Psychiatry.},
author = {Sotres-Bayon, Francisco and Cain, Christopher K. and LeDoux, Joseph E.},
doi = {10.1016/j.biopsych.2005.10.012},
file = {:Users/jasperhajonides/Library/Application Support/Mendeley Desktop/Downloaded/Sotres-Bayon, Cain, LeDoux - 2006 - Brain Mechanisms of Fear Extinction Historical Perspectives on the Contribution of Prefrontal Cortex.pdf:pdf},
isbn = {0006-3223 (Print)\n0006-3223 (Linking)},
issn = {00063223},
journal = {Biological Psychiatry},
keywords = {Emotion,amygdala,conditioning,exposure therapy,hippocampus,history},
number = {4},
pages = {329--336},
pmid = {16412988},
title = {{Brain Mechanisms of Fear Extinction: Historical Perspectives on the Contribution of Prefrontal Cortex}},
volume = {60},
year = {2006}
}
@article{Treisman1980,
abstract = {The study of specialized nutritional support using pharmaconutrients including glutamine, arginine, $\omega$-3 fatty acids, nucleic acids, and growth hormone has grown exponentially over the past several years. Recently, studies have shown that the use of glutamine can decrease bacterial translocation, support glutathione stores, and affect protein synthesis and degradation. Clinically, glutamine supplementation has been shown to reduce hospital stay. Arginine enhances nitrogen balance and has complex effects on the immune system, but evidence of clinical benefit is limited. $\omega$-3 Fatty acids, in the form of fish oil, reduce cholesterol and triglyceride levels and augment the inflammatory response; however, recent work suggests that their clinical effects on coronary artery disease are somewhat equivocal compared with earlier favorable reports. Growth hormone has been shown to enhance nitrogen balance, increase protein synthesis, and stimulate insulin-like growth factor 1 production. Combination regimens using these nutrients have shown promising effects on infectious complications and clinical outcome in specific patient subgroups. Continued research is ongoing to determine the indications and optimal use of these new pharmaconutrients.},
