issn = {1529-2401},
journal = {The Journal of Neuroscience},
keywords = {Acoustic Stimulation,Adult,Analysis of Variance,Auditory,Auditory Cortex,Auditory Cortex: physiology,Auditory Perception,Auditory Perception: physiology,Auditory: physiology,Brain Mapping,Contingent Negative Variation,Contingent Negative Variation: physiology,Evoked Potentials,Female,Humans,Magnetoencephalography,Male,Pitch Perception,Psychology,Reaction Time,Repression,Time Factors,Young Adult},
