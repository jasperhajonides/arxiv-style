@article{Nobre2014,
abstract = {Attention enables the selection of stimuli and responses for subsequent cognitive processing and the allocation of cognitive resources (focus), often for an extended period (sustained attention). Four elements of attention exist: (1) sensory selection; (2) focus and capacity; (3) response intention and executive attention; and (4) sustained attention. Disturbances of attention are among the most common symptoms observed in people with neurological and psychiatric disorders that affect the brain. Attention is invariably affected when there has been an alteration in consciousness or alertness, as in cases of delirium. One of the most dramatic attention disturbances, hemineglect syndrome, often occurs following unilateral stroke or tumor that affects the brain's attention system. Attentional impairments on clinical examination are very sensitive indicators of brain dysfunction, although not necessarily pathognomonic of a particular disease process.},
author = {Nobre, Anna C. and Kastner, Sabine},
doi = {10.1093/oxfordhb/9780199675111.013.040},
