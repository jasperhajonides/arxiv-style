keywords = {0351:Gerontology,0451:Social psychology,0633:Cognitive therapy,Age-related differences,Cognitive therapy,Gerontology,Inhibition,Life stress,Psychology,Social psychology,Social sciences,Working memory},
pages = {87--87 p.},
title = {{Age-related differences in the influence of positive and negative life stress on working memory}},
