title = {{Controversies in neuroscience: Topographic maps are fundamental to sensory processing}},
volume = {44},
year = {1997}
}
@article{DeYoe1996,
abstract = {Functional magnetic resonance imaging (fMRI) was used to identify and map the representation of the visual field in seven areas of human cerebral cortex and to identify at least two additional visually responsive regions. The cortical locations of neurons responding to stimulation along the vertical or horizontal visual field meridia were charted on three-dimensional models of the cortex and on unfolded maps of the cortical surface. These maps were used to identify the borders among areas that would be topographically homologous to areas V1, V2, V3, VP, and parts of V3A and V4 of the macaque monkey. Visually responsive areas homologous to the middle temporal/medial superior temporal area complex and unidentified parietal visual areas were also observed. The topography of the visual areas identified thus far is consistent with the organization in macaque monkeys. However, these and other findings suggest that human and simian cortical organization may begin to differ in extrastriate cortex at, or beyond, V3A and V4.},
author = {DeYoe, E A and Carman, G J and Bandettini, P and Glickman, S and Wieser, J and Cox, R and Miller, D and Neitz, J},
