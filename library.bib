pmid = {16753563},
title = {{Decoding Seen and Attended Motion Directions from Activity in the Human Visual Cortex}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S0960982206014643%5Cnpapers3://publication/doi/10.1016/j.cub.2006.04.003},
