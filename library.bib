journal = {Journal of Vision},
keywords = {choice repetition,confidence,decision-making,sequential choice bias,serial dependence},
number = {12},
pages = {1--13},
pmid = {33211062},
title = {{Opposite effects of choice history and evidence history resolve a paradox of sequential choice bias}},
volume = {20},
year = {2020}
}
